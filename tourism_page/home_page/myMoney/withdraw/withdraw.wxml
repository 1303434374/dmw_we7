<view class="recharge">
    <view class="recharge-tit">
        微信钱包
        <text class="tips">提现至</text>
    </view>
    <view class="recharge-cont">
        <view class="recharge-cont-tit">提现金额</view>
        <view class="price">
            <text>￥</text>
            <input bindinput="getPrice" maxlength="7" type="number" value="{{allIO?allPrice:''}}"></input>
        </view>
    </view>
    <view class="allPrice">
        <text style="color:#666;">余额￥{{allPrice}} </text>
        <text bindtap="allWithDraw"> 全部转出</text>
    </view>
    <view bindtap="send" class="sendBtn">确认提现</view>
</view>
