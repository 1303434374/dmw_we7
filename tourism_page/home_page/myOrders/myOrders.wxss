page {
    background: #f5f5f5;
}

.order-tit {
    width: 92vw;
    padding: 20rpx 4vw 0;
    background: #fff;
    border-bottom: 1px solid #666;
    font-size: 30rpx;
    white-space: nowrap;
}

.tit-item {
    padding: 0 0 20rpx;
    display: inline-block;
    text-align: center;
    margin-right: 68rpx;
}

.currentTit {
    color: #289Af6;
    border-bottom: 1px solid #289Af6;
}

.teamworkSuccess {
    position: relative;
    width: 100vw;
    padding: 20rpx 0;
    background: #fff;
    margin-top: 20rpx;
}

.userOperating {
    width: 92vw;
    padding: 0 4vw 20rpx;
    border-bottom: 2px solid #eaeaea;
    margin-bottom: 20rpx;
    color: #666;
    font-size: 32rpx;
    display: flex;
    justify-content: space-between;
}

.order {
    width: 92vw;
    padding: 0 4vw;
    display: flex;
    align-items: center;
}

.orderImg {
    width: 160rpx;
    height: 160rpx;
}

.orderInfor {
    font-size: 26rpx;
    margin: 0 0 0 20rpx;
    display: flex;
    flex-direction: column;
}

.order-options {
    margin: 10rpx 0;
}

.order-num {
    margin-bottom: 10rpx;
}

.timeAndStatus {
    width: 92vw;
    padding: 0 4vw;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    color: #777;
    font-size: 28rpx;
    margin-top: 20rpx;
}

.teamwork-btn {
    background: #fff;
    text-align: right;
    font-size: 28rpx;
}

.lookTeamwork {
    display: inline-block;
    border: 1px solid #bababa;
    border-radius: 20px;
    padding: 4rpx 24rpx;
    margin-left: 10rpx;
}

.notOrder {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 100vw;
    text-align: center;
    font-size: 32rpx;
    color: #666;
}

.notOrder>image {
    width: 150rpx;
    height: 158rpx;
}